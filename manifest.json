{
  "manifest_version": 3,
  "name": "PDF Bookmarks",
  "version": "1.3",
  "description": "Manage bookmarks for PDF pages with text anchors.",
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
